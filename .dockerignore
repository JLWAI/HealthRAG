*.tmp
*.bak
tmp/
temp/

# Environment variables (mounted at runtime)
.env
.env.local

# Docker files (not needed inside container)
Dockerfile
Dockerfile.homelab
docker-compose.yml
docker-compose.homelab.yml
.dockerignore

